{
  "name": "vite-react-ic-rust",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create backend && dfx generate backend && dfx deploy",
    "start": "run-p frontend",
    "frontend": "vite --port 3000",
    "backend": "cargo-watch -w 'src' -q -C ./backend -s 'cd .. && dfx generate && dfx deploy backend -q -y'",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" && cargo fmt"
  },
  "dependencies": {
    "@dfinity/agent": "^2.0.0",
    "@dfinity/auth-client": "^2.0.0",
    "@dfinity/candid": "^2.0.0",
    "@dfinity/cketh": "^3.3.2",
    "@dfinity/identity": "^2.2.0",
    "@dfinity/identity-secp256k1": "^2.0.0",
    "@dfinity/principal": "^2.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/style": "^0.8.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "@types/node": "^22.12.0",
    "@types/react-dom": "^19.0.3",
    "@types/react-dropzone": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "simple-cbor": "^0.4.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "dotenv": "^16.4.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "sass": "^1.57.1",
    "typescript": "^5.7.3",
    "vite": "^3.2.0",
    "vite-plugin-environment": "^1.1.3"
  }
}
